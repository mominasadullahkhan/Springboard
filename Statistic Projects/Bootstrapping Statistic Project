   "source": [
    "_ = plt.hist(mean_diff, bins= 30, alpha = 0.5, color = 'b')\n",
    "_ = plt.xlabel('Charges')\n",
    "_ = plt.axvline(np.mean(mean_diff),color='r', linestyle='--')\n",
    "_ = plt.axvline(np.mean(mean_diff_observed),color='g', linestyle=':')"
   ]
  },
